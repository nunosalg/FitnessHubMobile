<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FitnessHubMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FitnessHubMobile"
    xmlns:pages="clr-namespace:FitnessHubMobile.Pages"
    Shell.NavBarIsVisible="False"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarHasShadow="True"
    Shell.DisabledColor="#BFBFBF"
    Shell.TabBarBackgroundColor="{StaticResource FitnessHub}">

</Shell>
